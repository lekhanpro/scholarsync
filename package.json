{
  "name": "scholarsync",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\" \"npm run dev:worker\"",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "dev:worker": "cd server && npm run worker",
    "build": "cd client && npm run build && cd ../server && npm run build",
    "start": "cd server && npm start"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.1",
    "dotenv": "^16.4.7",
    "groq-sdk": "^0.8.0",
    "langchain": "^0.3.8",
    "multer": "^1.4.5-lts.1",
    "pdfjs-dist": "^4.10.38",
    "pdf-parse": "^1.1.1",
    "posthog-node": "^4.4.0",
    "tesseract.js": "^5.1.0",
    "canvas": "^2.11.2",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/multer": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "@vercel/node": "^5.6.4",
    "concurrently": "^9.1.0"
  }
}
